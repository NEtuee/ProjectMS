<AIPackage Name="AobaDischargePackage" DefaultState="AI_Discharge">

    <AIState>
        <AI_Discharge UpdateTime="0.0" TargetSearchType="Near" TargetSearchRange="999.0" SearchIdentifier="Player">
            <Event_OnExecute>
                <AIEvent Type="SetDirectionToTarget"/>
                <AIEvent Type="SetAction" Action="AC_Discharge"/>
            </Event_OnExecute>
            
            <Branch Condition="End" Execute="TerminatePackage"/>
        </AI_Discharge>

        <TerminatePackage>
            <Event_OnExecute>
                <AIEvent Type="TerminatePackage"/>
            </Event_OnExecute>
        </TerminatePackage>

    </AIState>

</AIPackage>
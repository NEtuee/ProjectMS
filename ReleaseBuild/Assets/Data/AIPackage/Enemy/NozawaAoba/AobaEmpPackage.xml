<AIPackage Name="AobaEmpPackage" DefaultState="AI_EmpReady">

    <AIState>
        <AI_EmpReady UpdateTime="0.0" TargetSearchType="Near" TargetSearchRange="999.0" SearchIdentifier="Player">
            <Event_OnExecute>
                <AIEvent Type="SetDirectionToTarget"/>
                <AIEvent Type="SetAction" Action="AC_EmpReady"/>
            </Event_OnExecute>
            
            <Event_OnUpdate>
                <AIEvent Type="SetDirectionToTarget"/>
            </Event_OnUpdate>
            
            <Branch Condition="End" Execute="AI_EmpThrow"/>
        </AI_EmpReady>

        <AI_EmpThrow UpdateTime="0.0" TargetSearchType="Near" TargetSearchRange="999.0" SearchIdentifier="Player">
            <Event_OnExecute>
                <AIEvent Type="SetAction" Action="AC_EmpThrow"/>
            </Event_OnExecute>

            <Branch Condition="End" Execute="TerminatePackage"/>          
        </AI_EmpThrow>

        <TerminatePackage>
            <Event_OnExecute>
                <AIEvent Type="TerminatePackage"/>
            </Event_OnExecute>
        </TerminatePackage>

    </AIState>

</AIPackage>
    <Hit MovementType="FrameEvent" DirectionType="MousePoint" DefenceDirectionType="MousePoint" FlipType="Keep" DefenceType="Guard" DefenceAngle="180.0">
        <Animation Path="Resources/Sprites/NewPlayer/Hit/" FramePerSecond="2" XFlip="true">
            <MultiSelectAnimation Path="Resources/Sprites/NewPlayer/empty" Condition="IsCatchTarget"/>
            <FrameEvent Type="Movement" StartFrame="0" Friction="gv_IdleFriction" MaxVelocity="0" Velocity="0" Speed="0"/>
        </Animation>

        <!-- <Branch Condition="getStat_IsStun == 1.0" Execute="Stun"/> -->
        <Branch Condition="IsCatchTarget" Execute="Catched"/>
        <Branch Condition="End" Execute="Idle"/>
        <Branch Key="(getKey_Guard == false) && ((getKey_AttackCharge == true) || (getKey_NormalMove == true))" Condition="End" Execute="MoveAll"/>
        <Branch Key="(getKey_Guard == false) && (getKey_AttackCharge == false)" Condition="End" Execute="Idle"/>
    </Hit>

    <Kiiled DirectionType="Keep" FlipType="Direction">
        <Animation Path="Resources/Sprites/DummyPlayer/Hit/" StartFrame="0" EndFrame="1" FramePerSecond="99" XFlip="true">
            <FrameEvent Type="KillEntity" StartFrame="0"/>
        </Animation>

        <Branch Condition="End == true" Execute="Kiiled"/>
    </Kiiled>

    <Catched MovementType="FrameEvent" DirectionType="AttackedPoint" FlipType="Direction" ApplyBuff="CatchedEscape">
        <Animation Path="Resources/Sprites/NewPlayer/empty" StartFrame="0" EndFrame="1" XFlip="true">
            <FrameEvent Type="Test" Log="Akane Catched"/>
            <FrameEvent Type="Movement" StartFrame="0" Friction="12" MaxVelocity="0" Velocity="0" Speed="0"/>
            <FrameEvent Type="ApplyBuff" StartFrame="0" ApplyBuff="StunClear"/>
            <FrameEvent Type="FrameTag" StartFrame="0" EndFrame="999" Tag="Recatchable" Condition="getStat_IsCatched == 1.0"/>
            <FrameEvent Type="FrameTag" StartFrame="0" EndFrame="999" Tag="Catched"/>
            <FrameEvent Type="ReleaseCatch" Push="4 0 0" Condition="getStat_IsCatched == 0.0"/>
        </Animation>

        <Branch Condition="IsCatchTarget == false" Execute="CatchReleased"/>
        <Branch Condition="End == true" Execute="Catched"/>
    </Catched>

    <CatchReleased MovementType="FrameEvent" DirectionType="AttackedPoint" FlipType="Direction" RotationType="Direction">
        <Animation Preset="Sprites/NewPlayer/catchRealese/" XFlip="true" Duration="1.3">
            <FrameEvent Type="ApplyBuff" StartFrame="0" ApplyBuff="CatchedClear"/>
            <FrameEvent Type="ApplyBuff" StartFrame="0" ApplyBuff="StunClear"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="1.3" Tag="CatchReleased"/>
            <FrameEvent Type="FrameTag" StartTime="0.7" EndFrame="1.3" Tag="MoveCancel"/>
        </Animation>
        
        <!-- <Animation Path="Resources/Sprites/NewPlayer/catchRealese/" FramePerSecond="10" XFlip="true"> -->
            <!-- 1.3 sec -->
            <!-- <FrameEvent Type="SetAnimationSpeed" StartFrame="0" Speed="0.1428"/> --> <!-- Delay Motion: 0.7 sec -->
            <!-- <FrameEvent Type="SetAnimationSpeed" StartFrame="1" Speed="0.1666"/> --> <!-- Cancel Available Motion: 0.6 sec -->
        <!-- </Animation> -->
        
        <UseBranchSet Name="CancelBranch"/>

        <Branch Condition="End" Execute="Idle"/>
    </CatchReleased>

    <Stun MovementType="FrameEvent" DirectionType="Keep" FlipType="Direction" ApplyBuff="StunEscape">
        <Animation Preset="Sprites/NewPlayer/stun" XFlip="true" Duration="3.0">
            <FrameEvent Type="FrameTag" StartTime="0" EndFrame="999" Tag="Stun"/>
            <FrameEvent Type="ApplyBuff" StartTime="3.0" ApplyBuff="StunClear"/>
        </Animation>

        <Branch Condition="getStat_IsStun == 0.0" Execute="Idle"/>
    </Stun>

</ActionGraph>
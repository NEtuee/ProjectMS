<ActionGraph Name="SisterStaffAction" DefaultAction="AC_Idle" DefaultFramePerSecond="12">

<!-- AnimationHeader MasterPath -->
<GlobalVariable Name="gv_MasterPath" Value="Sprites/Enemy/SisterStaff/"/>
<GlobalVariable Name="gv_DeadPath" Value="Resources/Sprites/Enemy/SisterStaff/0_default/4_dead"/>

<!-- Defualt Action -->
    <AC_Idle MovementType="FrameEvent" DirectionType="AI" FlipType="Direction">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="0_default/1_idle" XFlip="true" Duration="1">
            <FrameEvent Type="Movement" StartTime="0" Friction="12" Speed="0"/>
            
            <FrameEvent Type="ApplyBuff" StartTime="0" ApplyBuff="HitCountClear"/>
            <FrameEvent Type="ApplyBuff" StartTime="0" ApplyBuff="GuardCountClear"/>

            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="HitAvail"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>
            
        </Animation>
        <Trigger Condition="(DefenceSuccess || GuardBreakFail || GuardBroken) && (customValue_IsBarriorOn == 1.0) ">
            <FrameEvent Type="DeleteBuff" StartTime="0" BuffList="HelenaBarriorOn"/>
            <FrameEvent Type="AudioPlay" Condition="customValue_IsBarriorOn == 1.0" StartTime="0" ID="2512" Attach="true" Switch="True"/>
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="BarriorBroken" EventTargetType="Self"/>
        </Trigger>

        <Branch Condition="End == true" Execute="AC_Idle"/>
    </AC_Idle>

    <AC_Move MovementType="FrameEvent" DirectionType="AI" FlipType="Direction">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="0_default/2_move" XFlip="true" Duration="0.5">
            <FrameEvent Type="Movement" StartTime="0" Friction="0" MaxVelocity="5" Speed="10"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="HitAvail"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>
        </Animation>
        <Trigger Condition="(DefenceSuccess || GuardBreakFail || GuardBroken) && (customValue_IsBarriorOn == 1.0) ">
            <FrameEvent Type="DeleteBuff" StartTime="0" BuffList="HelenaBarriorOn"/>
            <FrameEvent Type="AudioPlay" Condition="customValue_IsBarriorOn == 1.0" StartTime="0" ID="2512" Attach="true" Switch="True"/>
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="BarriorBroken" EventTargetType="Self"/>
        </Trigger>

        <Branch Condition="End == true" Execute="AC_Move"/>
    </AC_Move>

    <AC_Hit MovementType="FrameEvent" DirectionType="AttackedPoint" FlipType="Direction">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="0_default/3_hit" XFlip="true" Duration="1.25">
            <FrameEvent Type="Movement" StartTime="0" Friction="0" Speed="0" Velocity="0"/>
            <FrameEvent Type="ApplyBuff" StartTime="0" ApplyBuff="HitCountIncrease"/>
            
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="CancelLaser" EventTargetType="SummonTargetRange" Range="999.0" AllyTarget="Ally"/>
            
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="HitAvail"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>
        </Animation>
        <Trigger Condition="(DefenceSuccess || GuardBreakFail || GuardBroken) && (customValue_IsBarriorOn == 1.0) ">
            <FrameEvent Type="DeleteBuff" StartTime="0" BuffList="HelenaBarriorOn"/>
            <FrameEvent Type="AudioPlay" Condition="customValue_IsBarriorOn == 1.0" StartTime="0" ID="2512" Attach="true" Switch="True"/>
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="BarriorBroken" EventTargetType="Self"/>
        </Trigger>

        <Branch Condition="End == true" Execute="AC_Idle"/>
    </AC_Hit>

    <Dead DirectionType="Keep" FlipType="Direction">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="0_default/4_dead" XFlip="true" Duration="1">
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="CancelLaser" EventTargetType="SummonTargetRange" Range="999.0" AllyTarget="Ally"/>

            <FrameEvent Type="Effect" StartTime="0" Path="gv_DeadPath" FramePerSecond="0.2" CastShadow="True">
                <Physics UseGravity="True" Velocity="Random_-2.5^2.5 Random_1.5^3.5" Friction="1" Torque="Random_-30^30" AngularFriction="10.0"/>
            </FrameEvent>

            <FrameEvent Type="KillEntity" StartTime="0"/>
        </Animation>

        <Branch Condition="End == true" Execute="Dead"/>
    </Dead>
    
    <AC_PeaceMotionLoop MovementType="FrameEvent" DirectionType="Keep" FlipType="Direction">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="0_default/5_peace/1_loop" XFlip="true" Duration="1">
            <FrameEvent Type="Movement" StartTime="0" Friction="12" Speed="0"/>
            
            <FrameEvent Type="ApplyBuff" StartTime="0" ApplyBuff="HitCountClear"/>
            <FrameEvent Type="ApplyBuff" StartTime="0" ApplyBuff="GuardCountClear"/>

            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="HitAvail"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>
            
        </Animation>

        <Branch Condition="End == true" Execute="AC_Idle"/>
    </AC_PeaceMotionLoop>
    <AC_PeaceMotionEnd MovementType="FrameEvent" DirectionType="AI" FlipType="Direction">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="0_default/5_peace/2_end" XFlip="true" Duration="1">
            <FrameEvent Type="Movement" StartTime="0" Friction="12" Speed="0"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="HitAvail"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>
            
        </Animation>

        <Branch Condition="End == true" Execute="AC_Idle"/>
    </AC_PeaceMotionEnd>
<!-- Defualt Action -->

<!-- Skill : Pray -->
    <AC_Pray MovementType="FrameEvent" DirectionType="AI" FlipType="Direction">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="2_skill/1_pray" XFlip="true" Duration="2.0">
            <ActionFlag StartTime="0" EndTime="1.5" Flags="OutlineNormal"/>
            <FrameEvent Type="Movement" StartTime="0" Friction="12" MaxVelocity="0" Speed="0"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>
            <FrameEvent Type="AudioPlay" Condition="getWeight_RandVector3^x" StartTime="0" ID="2509" Attach="true" Switch="True"/>
            <FrameEvent Type="AudioPlay" StartTime="1.5" ID="2510" Attach="true" Switch="True"/>
            <FrameEvent Type="FollowAttack" StartTime="0" EndTime="1.5" FollowType="Attach" Radius="2.0" MoveTime="1.5" CharacterKey="SisterStaffCrossHair" ToTarget="true">
                <OnBegin>
                </OnBegin>
                <OnEnd>
                    <FrameEvent Type="Projectile" StartTime="1.5" GraphName="ThunderStorm_Large" DirectionType="Keep" SpawnTargetType="Self"/>
                </OnEnd>
            </FrameEvent>
        </Animation>
        <Trigger Condition="(DefenceSuccess || GuardBreakFail || GuardBroken) && (customValue_IsBarriorOn == 1.0) ">
            <FrameEvent Type="DeleteBuff" StartTime="0" BuffList="HelenaBarriorOn"/>
            <FrameEvent Type="AudioPlay" Condition="customValue_IsBarriorOn == 1.0" StartTime="0" ID="2512" Attach="true" Switch="True"/>
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="BarriorBroken" EventTargetType="Self"/>
        </Trigger>

        <Branch Condition="End == true" Execute="AC_Idle"/>
        <Branch Condition="End" Execute="AC_Idle"/>
    </AC_Pray>
<!-- Skill : Pray -->
<!-- Skill : Tracer -->
    <AC_TracerReady MovementType="FrameEvent" DirectionType="AI" FlipType="Direction" Flags="OutlineNormal ActionFlow">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="2_skill/2_tracer/1_ready" XFlip="true" Duration="0.5">
            <FrameEvent Type="Movement" StartTime="0" Friction="12" Speed="0"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>
        </Animation>
        <Trigger Condition="(DefenceSuccess || GuardBreakFail || GuardBroken) && (customValue_IsBarriorOn == 1.0) ">
            <FrameEvent Type="DeleteBuff" StartTime="0" BuffList="HelenaBarriorOn"/>
            <FrameEvent Type="AudioPlay" Condition="customValue_IsBarriorOn == 1.0" StartTime="0" ID="2512" Attach="true" Switch="True"/>
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="BarriorBroken" EventTargetType="Self"/>
        </Trigger>

        <Branch Condition="End" Execute="AC_TracerSpawn"/>
    </AC_TracerReady>

    <AC_TracerSpawn MovementType="FrameEvent" DirectionType="AI" FlipType="Direction">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="2_skill/2_tracer/2_spawn" XFlip="true" Duration="0.35">
            <ActionFlag StartTime="0" EndTime="999" Flags="OutlineNormal"/>
            <FrameEvent Type="AudioPlay" StartTime="0" ID="2204" Attach="true"/>
            <FrameEvent Type="Movement" StartTime="0" Friction="18" MaxVelocity="10" Velocity="0" Speed="0"/>            
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="TracerSpawn" EventTargetType="Self"/>
            <FrameEvent Type="SpawnCharacter" StartTime="0" CharacterKey="Obj_SisterTracer" Offset="0 0 0" UseFlip="true"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>
        </Animation>
        <Animation Preset="2_skill/2_tracer/3_recover" XFlip="true" Duration="1.2">
            <FrameEvent Type="Movement" StartTime="0" Friction="12" Speed="0"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="HitAvail"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>
        </Animation>
        <Trigger Condition="(DefenceSuccess || GuardBreakFail || GuardBroken) && (customValue_IsBarriorOn == 1.0) ">
            <FrameEvent Type="DeleteBuff" StartTime="0" BuffList="HelenaBarriorOn"/>
            <FrameEvent Type="AudioPlay" Condition="customValue_IsBarriorOn == 1.0" StartTime="0" ID="2512" Attach="true" Switch="True"/>
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="BarriorBroken" EventTargetType="Self"/>
        </Trigger>

        <Branch Condition="End" Execute="AC_Idle"/>
    </AC_TracerSpawn>
<!-- Skill : Tracer -->
<!-- Skill : Swing -->
    <AC_AttackReady MovementType="FrameEvent" DirectionType="AI" FlipType="Direction" Flags="OutlineNormal">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="2_skill/3_swing/1_attackReady" XFlip="true" Duration="0.5">            
            <FrameEvent Type="Movement" StartTime="0" Friction="0.0" MaxVelocity="5.0" Speed="10.0"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>

        </Animation>
        <Trigger Condition="(DefenceSuccess || GuardBreakFail || GuardBroken) && (customValue_IsBarriorOn == 1.0) ">
            <FrameEvent Type="DeleteBuff" StartTime="0" BuffList="HelenaBarriorOn"/>
            <FrameEvent Type="AudioPlay" Condition="customValue_IsBarriorOn == 1.0" StartTime="0" ID="2512" Attach="true" Switch="True"/>
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="BarriorBroken" EventTargetType="Self"/>
        </Trigger>
        <Branch Condition="End" Execute="AC_Idle"/>
    </AC_AttackReady>

    <AC_AttackMain MovementType="FrameEvent" DirectionType="AI" FlipType="Direction" RotationType="Direction" Flags="OutlineNormal">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="2_skill/3_swing/2_attackMain" XFlip="true" Duration="0.55">
            <FrameEvent Type="AudioPlay" StartTime="0" ID="4901" Attach="true"/>
            <FrameEvent Type="Movement" StartTime="0" Friction="18.0" MaxVelocity="10.0" Velocity="10.0" Speed="0.0"/>
            <!-- TODO : AttakcPreset 변경 -->
            <FrameEvent Type="Attack" StartTime="0" EndTime="0.2" AttackPreset="Sports_BatterSwing">
                <OnHit>
                    <FrameEvent Type="Movement" StartTime="0" Friction="24.0" MaxVelocity="5.0" Velocity="0.0" Speed="0.0"/>

                    <FrameEvent Type="AudioPlay" StartTime="0" ID="4902" Attach="true"/>
                    <FrameEvent Type="ApplyBuffTarget" ApplyBuff="CommonAttack"/>

                    <FrameEvent Type="EffectPreset" StartTime="0" Key="Slash"/>
                    <FrameEvent Type="EffectPreset" StartTime="0" Key="Blood"/>
                    
                    <FrameEvent Type="StopUpdate" Time="0.1"/>
                    <FrameEvent Type="SetTimeScale" Scale="0.1" Time="0.1" BlendTime="0.1"/>
                    <FrameEvent Type="ShakeEffect" Scale="0.1" Speed="10.0" Time="0.1"/>
                </OnHit>
            </FrameEvent>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>
        </Animation>
        <Trigger Condition="(DefenceSuccess || GuardBreakFail || GuardBroken) && (customValue_IsBarriorOn == 1.0) ">
            <FrameEvent Type="DeleteBuff" StartTime="0" BuffList="HelenaBarriorOn"/>
            <FrameEvent Type="AudioPlay" Condition="customValue_IsBarriorOn == 1.0" StartTime="0" ID="2512" Attach="true" Switch="True"/>
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="BarriorBroken" EventTargetType="Self"/>
        </Trigger>

        <Branch Condition="End" Execute="AC_AttackRecover"/>
    </AC_AttackMain>

    <AC_AttackRecover MovementType="FrameEvent" DirectionType="AI" FlipType="Direction">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="2_skill/3_swing/3_attackRecover" XFlip="true" Duration="1.2">
            <FrameEvent Type="Movement" StartTime="0" Friction="12" Speed="0"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="HitAvail"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>
        </Animation>
        <Trigger Condition="(DefenceSuccess || GuardBreakFail || GuardBroken) && (customValue_IsBarriorOn == 1.0) ">
            <FrameEvent Type="DeleteBuff" StartTime="0" BuffList="HelenaBarriorOn"/>
            <FrameEvent Type="AudioPlay" Condition="customValue_IsBarriorOn == 1.0" StartTime="0" ID="2512" Attach="true" Switch="True"/>
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="BarriorBroken" EventTargetType="Self"/>
        </Trigger>

        <Branch Condition="End" Execute="AC_Idle"/>
    </AC_AttackRecover>
<!-- Skill : Swing -->
<!-- Skill : Barrior -->
    <AC_Barrior MovementType="FrameEvent" DirectionType="AI" FlipType="Direction">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="2_skill/4_barrior" XFlip="true" Duration="1.5">
            <ActionFlag StartTime="0" EndTime="1" Flags="OutlineNormal"/>

            <FrameEvent Type="AudioPlay" StartTime="0" ID="2509" Attach="true" Switch="True"/>
            <FrameEvent Type="Movement" StartTime="0" Friction="12" Speed="0"/>

            <FrameEvent Type="ApplyBuff" StartTime="1" ApplyBuff="HelenaBarriorOn"/>
            <FrameEvent Type="CallAIEvent" StartTime="1" EventName="BarriorActivated" EventTargetType="Self"/>
            <FrameEvent Type="AudioPlay" StartTime="1" ID="2510" Attach="true" Switch="True"/>

            <FrameEvent Type="ApplyBuff" StartTime="0" ApplyBuff="HitCountClear"/>
            <FrameEvent Type="ApplyBuff" StartTime="0" ApplyBuff="GuardCountClear"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>
            <FrameEvent Type="FrameTag" StartTime="1" EndTime="999" Tag="HitAvail"/>
        </Animation>
        <Trigger Condition="(DefenceSuccess || GuardBreakFail || GuardBroken) && (customValue_IsBarriorOn == 1.0) ">
            <FrameEvent Type="DeleteBuff" StartTime="0" BuffList="HelenaBarriorOn"/>
            <FrameEvent Type="AudioPlay" Condition="customValue_IsBarriorOn == 1.0" StartTime="0" ID="2512" Attach="true" Switch="True"/>
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="BarriorBroken" EventTargetType="Self"/>
        </Trigger>

        <Branch Condition="End == true" Execute="AC_Idle"/>
        <Branch Condition="End" Execute="AC_Idle"/>
    </AC_Barrior>
<!-- Skill : Barrior -->

<!-- Defense Action -->
    <AC_Guard MovementType="FrameEvent" DirectionType="AI" FlipType="Direction" DefenceType="Guard" DefenceDirectionType="Direction" DefenceAngle="180">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="1_defense/1_guard" XFlip="true" Duration="0.5">
            <FrameEvent Type="Movement" StartTime="0" Friction="0" Speed="0" Velocity="0"/>
            <FrameEvent Type="ApplyBuff" Condition="DefenceSuccess" StartTime="0" EndTime="999" ApplyBuff="GuardCountIncrease"/>

            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="HitAvail"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>
        </Animation>
        <Trigger Condition="(DefenceSuccess || GuardBreakFail || GuardBroken) && (customValue_IsBarriorOn == 1.0) ">
            <FrameEvent Type="DeleteBuff" StartTime="0" BuffList="HelenaBarriorOn"/>
            <FrameEvent Type="AudioPlay" Condition="customValue_IsBarriorOn == 1.0" StartTime="0" ID="2512" Attach="true" Switch="True"/>
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="BarriorBroken" EventTargetType="Self"/>
        </Trigger>

        <Branch Condition="End == true" Execute="AC_Guard"/>
    </AC_Guard>

    <AC_GuardBroken MovementType="FrameEvent" DirectionType="AttackedPoint" FlipType="Direction">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="1_defense/2_guardBroken" XFlip="true" Duration="1.25">
            <FrameEvent Type="DeleteBuff" StartTime="0" BuffList="HelenaBarriorOn"/>
            <FrameEvent Type="AudioPlay" Condition="customValue_IsBarriorOn == 1.0" StartTime="0" ID="2512" Attach="true" Switch="True"/>

            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="BarriorBroken" EventTargetType="Self"/>
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="CancelLaser" EventTargetType="SummonTargetRange" Range="999.0" AllyTarget="Ally"/>

            <FrameEvent Type="AudioPlay" StartTime="0" ID="4921" Attach="true"/>
            <FrameEvent Type="Movement" StartTime="0" Friction="0" Speed="0" Velocity="0"/>
            <FrameEvent Type="ApplyBuff" StartTime="0" ApplyBuff="HitCountIncrease"/>
            <FrameEvent Type="ApplyBuff" StartTime="0" ApplyBuff="GuardCountClear"/>

            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="HitAvail"/>
            <FrameEvent Type="FrameTag" StartTime="0" EndTime="999" Tag="GbAvail"/>
        </Animation>
        <Trigger Condition="(DefenceSuccess || GuardBreakFail || GuardBroken) && (customValue_IsBarriorOn == 1.0) ">
            <FrameEvent Type="DeleteBuff" StartTime="0" BuffList="HelenaBarriorOn"/>
            <FrameEvent Type="AudioPlay" Condition="customValue_IsBarriorOn == 1.0" StartTime="0" ID="2512" Attach="true" Switch="True"/>
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="BarriorBroken" EventTargetType="Self"/>
        </Trigger>

        <Branch Condition="End == true" Execute="AC_Idle"/>
    </AC_GuardBroken>

    <AC_Dodge MovementType="GraphPreset" MovementGraphPreset="Helena_BackStep" DirectionType="AI" FlipType="Direction" RotationType="Direction">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="1_defense/3_dodge" XFlip="true" Duration="0.35">
            <FrameEvent Type="AudioPlay" StartTime="0" ID="2100" Attach="true"/>

            <FrameEvent Type="ApplyBuff" StartTime="0" ApplyBuff="HitCountClear"/>
            <FrameEvent Type="ApplyBuff" StartTime="0" ApplyBuff="GuardCountClear"/>
        </Animation>
        <Trigger Condition="(DefenceSuccess || GuardBreakFail || GuardBroken) && (customValue_IsBarriorOn == 1.0) ">
            <FrameEvent Type="DeleteBuff" StartTime="0" BuffList="HelenaBarriorOn"/>
            <FrameEvent Type="AudioPlay" Condition="customValue_IsBarriorOn == 1.0" StartTime="0" ID="2512" Attach="true" Switch="True"/>
            <FrameEvent Type="CallAIEvent" StartTime="0" EventName="BarriorBroken" EventTargetType="Self"/>
        </Trigger>
        
        <Branch Condition="End" Execute="AC_Idle"/>

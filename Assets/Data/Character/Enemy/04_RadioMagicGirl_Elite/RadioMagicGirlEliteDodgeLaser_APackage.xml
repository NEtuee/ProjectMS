<AIPackage Name="RadioMagicGirlEliteDodgeLaser_APackage" DefaultState="AI_Entry">

    <AIState>
        <AI_Entry UpdateTime="0.0">
            <Event_OnExecute>
                <AIEvent Type="SetAction" Action="AC_Idle"/>
            </Event_OnExecute>
            <Branch Condition="true" Execute="AI_DodgeReady_A"/>
        </AI_Entry>

        <AI_DodgeReady_A UpdateTime="0.0">
            <Event_OnExecute>
                <AIEvent Type="SetAction" Action="AC_DodgeLaserTPReady_A"/>
            </Event_OnExecute>
            <Branch Condition="true" Execute="AI_Dodge_A"/>
        </AI_DodgeReady_A>

        <AI_Dodge_A UpdateTime="0.0" TargetSearchType="Near" TargetSearchRange="999.9" AllyTarget="Enemy">
            <Event_OnExecute>
                <AIEvent Condition="AngleDirectionToTarget < 180.0" Type="SetDirectionToTarget" Angle="150"/>
                <AIEvent Condition="AngleDirectionToTarget >= 180.0" Type="SetDirectionToTarget" Angle="210"/>
                <AIEvent Type="SetAction" Action="AC_DodgeLaserTP_A"/>
            </Event_OnExecute>

            <Branch Condition="End || getFrameTag_Cancel" Execute="AI_DodgeLaserReady_A"/>
        </AI_Dodge_A>

        <AI_DodgeLaserReady_A UpdateTime="0.0" TargetSearchType="Near" TargetSearchRange="999.0" AllyTarget="Enemy">
            <Event_OnExecute>
                <AIEvent Type="SetDirectionToTarget"/>
                <AIEvent Type="SetAction" Action="AC_DodgeLaserMain_A"/>
            </Event_OnExecute>

            <Event_OnUpdate>
                <AIEvent Type="SetDirectionToTarget" Speed="400"/>
            </Event_OnUpdate>
            
            <Branch Condition="End" Execute="AI_DodgeLaserMain_A"/>
        </AI_DodgeLaserReady_A>

        <AI_DodgeLaserMain_A UpdateTime="0.0" TargetSearchType="Near" TargetSearchRange="999.0" AllyTarget="Enemy">
            <Event_OnExecute>
                <AIEvent Type="SetDirectionToTarget"/>
                <AIEvent Type="SetAction" Action="AC_DodgeLaserMain_A"/>
            </Event_OnExecute>
            
            <Branch Condition="End" Execute="AI_Dodge_A"/>
        </AI_DodgeLaserMain_A>

        <AI_DodgeReady_B UpdateTime="0.0">
            <Event_OnExecute>
                <AIEvent Type="SetAction" Action="AC_DodgeLaserTPReady_B"/>
            </Event_OnExecute>
            <Branch Condition="true" Execute="AI_Dodge_B"/>
        </AI_DodgeReady_B>

        <AI_Dodge_B UpdateTime="0.0" TargetSearchType="Near" TargetSearchRange="999.9" AllyTarget="Enemy">
            <Event_OnExecute>
                <AIEvent Condition="AngleDirectionToTarget < 180.0" Type="SetDirectionToTarget" Angle="150"/>
                <AIEvent Condition="AngleDirectionToTarget >= 180.0" Type="SetDirectionToTarget" Angle="210"/>
                <AIEvent Type="SetAction" Action="AC_DodgeLaserTP_B"/>
            </Event_OnExecute>

            <Branch Condition="End || getFrameTag_Cancel" Execute="AI_DodgeLaserReady_B"/>
        </AI_Dodge_B>

        <AI_DodgeLaserReady_B UpdateTime="0.0" TargetSearchType="Near" TargetSearchRange="999.0" AllyTarget="Enemy">
            <Event_OnExecute>
                <AIEvent Type="SetDirectionToTarget"/>
                <AIEvent Type="SetAction" Action="AC_DodgeLaserMain_B"/>
            </Event_OnExecute>

            <Event_OnUpdate>
                <AIEvent Type="SetDirectionToTarget" Speed="400"/>
            </Event_OnUpdate>
            
            <Branch Condition="End" Execute="AI_DodgeLaserMain_B"/>
        </AI_DodgeLaserReady_B>

        <AI_DodgeLaserMain_B UpdateTime="0.0" TargetSearchType="Near" TargetSearchRange="999.0" AllyTarget="Enemy">
            <Event_OnExecute>
                <AIEvent Type="SetDirectionToTarget"/>
                <AIEvent Type="SetAction" Action="AC_DodgeLaserMain_B"/>
            </Event_OnExecute>
            
            <Branch Condition="End" Execute="AI_AttackRecover"/>
        </AI_DodgeLaserMain_B>

        <AI_AttackRecover UpdateTime="0.0" TargetSearchType="Near" TargetSearchRange="999.0" AllyTarget="Enemy">
            <Event_OnExecute>
                <AIEvent Type="SetAction" Action="AC_DodgeLaserRecover"/>
            </Event_OnExecute>
            
            <Branch Condition="End" Execute="TerminatePackage"/>
        </AI_AttackRecover>

        <TerminatePackage>
            <Event_OnExecute>
                <AIEvent Type="TerminatePackage"/>
            </Event_OnExecute>
        </TerminatePackage>

    </AIState>

</AIPackage>
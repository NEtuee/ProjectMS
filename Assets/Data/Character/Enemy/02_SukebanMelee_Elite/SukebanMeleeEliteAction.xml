<!-- Skill : Tsuba -->

    <AC_Tsuba MovementType="FrameEvent" DirectionType="AI" FlipType="Direction">
        <AnimationHeader MasterPath="gv_MasterPath"/>
        <Animation Preset="2_skill/5_tsuba/1_entry" XFlip="true" Duration="0.3">

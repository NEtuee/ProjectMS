<AIGraph Name="TransparentBindingBulletAI" DefaultState="Idle">

    <Include Path="Assets/Data/AILibrary/Package/CommonIdlePackage.xml"/>
    <Include Path="Assets/Data/AILibrary/Package/CommonDeadPackage.xml"/>
    <Include Path="Assets/Data/Character/Object/TransparentBindingBullet/TransparentBindingBulletPackage.xml"/>

    <Idle Package="CommonIdlePackage">
        <Branch Condition="Dead" Execute="Dead"/>
        <Branch Condition="GraphExecutedTime >= 0.2" Execute="Shot"/>
    </Idle>

    <Shot Package="TransparentBindingBulletPackage">
        <Branch Condition="Dead" Execute="Dead"/>
    </Shot>

    <Dead Package="CommonDeadPackage">
        <Branch Condition="Dead" Execute="Dead"/>
    </Dead>

</AIGraph>
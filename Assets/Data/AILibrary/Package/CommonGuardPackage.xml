<AIPackage Name="CommonGuardPackage" DefaultState="AI_Guard">

    <AIState>
        <AI_Guard UpdateTime="0.0">
            <Event_OnExecute>
                <AIEvent Type="SetAction" Action="AC_Guard"/>
            </Event_OnExecute>
            <Branch Condition="End == true" Execute="AI_Terminate"/>
        </AI_Guard>

        <AI_Terminate UpdateTime="0.0">
            <Event_OnExecute>
                <AIEvent Type="TerminatePackage"/>
            </Event_OnExecute>
        </AI_Terminate>

    </AIState>

</AIPackage>